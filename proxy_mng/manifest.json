{
    "name": "Better Nike Bot Helper",
    "version": "0.0.1",
    "manifest_version": 2,
    "description": "Powered by BetterNikeBot.com",
    "homepage_url": "http://www.betternikebot.com",
    "background": {
        "scripts": [
            "background.js"
        ],
        "persistent": true
    },
    "browser_action": {
        "default_title": "Better Nike Bot"
    },
    "permissions": [
        "proxy",
        "tabs",
        "cookies",
        "webRequest",
        "webRequestBlocking",
        "<all_urls>"
    ]
}